pkgname=ix
pkgver=0.1
pkgrel=1
pkgdesc="A ix client command line."
arch=('i686' 'x86_64')
url="https://github.com/PedroSFreitas/ix-client"
license=('unknown')
depends=('python2')
source=("https://github.com/PedroSFreitas/ix-client/blob/master/PKGBUILD/$pkgname-$pkgver.tar.gz")
md5sums=('SKIP')

package() {
	install -dm755 $pkgname "$pkgdir/usr/bin/$pkgname"
}
